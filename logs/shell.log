/* mobile dropdown */

#phone-drop-down-container {

	position: absolute;

	top: 135px;

	width: 100%;

	height: 314px;

	background: #FFFFFF;

	z-index: 999;

	overflow: hidden;

	-webkit-transition: height .5s ease-in-out;

	-moz-transition: height .5s ease-in-out;

	-ms-transition: height .5s ease-in-out;

	-o-transition: height .5s ease-in-out;

	transition: height .5s ease-in-out;

	-webkit-box-shadow: 0 5px 10px -5px #000;

	box-shadow: 0 5px 10px -5px #000;

	border-radius: .1px;

}



#phone-drop-down-container.closed {

	height: 0;

}



#phone-drop-down-container #phone-drop-down-wrap {

	padding: 15px;

}



#phone-drop-down-container #phone-drop-down-wrap .phone-wrap {

	margin-bottom: 20px;

}



#phone-drop-down-container #phone-drop-down-wrap .phone-wrap.last {

	margin-bottom: 0px;

}



#phone-drop-down-container #phone-drop-down-wrap .phone-title {

	font-size: 17px;

	display: block;

}



#phone-drop-down-container #phone-drop-down-wrap a {

	text-decoration: none;

	color: #fff;

}



#phone-drop-down-container #phone-drop-down-wrap .phone-number {

	font-size: 14px;

	display: block;

	background: #32332d;

	color: #fff;

	padding: 15px;

	border-radius: 5px;

	margin-top: 5px;

}



#phone-drop-down-container #phone-drop-down-wrap .phone-number:hover {

	background: #da3905;

}



#phone-drop-down-container #phone-drop-down-close-wrap {

	text-align: center;

	font-size: 15px;

	display: block;

	background: #cecece;

	color: #444444;

	border-top: 1px solid #777777;

	padding: 15px;

	cursor: pointer;

}



